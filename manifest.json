{
    "name": "NovoSGA",
    "id": "NovoSGA",
    "packaging_format": 1,
    "description": {
        "en": "Queue Management System.",
        "fr": "systÃ¨me de gestion de file d'attente  (QMS)."
    },
    "version": "2.1.0~ynh1",
    "url": "http://novosga.org/",
    "upstream": {
        "license": "MIT License",
        "website": "http://novosga.org/",
        "admindoc": "https://glpi-install.readthedocs.io/en/latest/",
        "code": "https://github.com/novosga/novosga"
    },
    "license": "MIT License",
    "maintainer": {
        "name": "rogeriolino",
        "email": "rogeriolino@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.3-fpm",
        "mysql"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/novosga",
                "default": "/novosga"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
